<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xrgx.courseonline.mapper.TeacherMapper">
    <select id="findTeacherByTid" parameterType="string" resultType="teacher">
        select * from tab_teacher
        where tid = #{tid}
    </select>
</mapper>